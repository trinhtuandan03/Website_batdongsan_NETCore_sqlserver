@model WebBatDongSan_DKCN.ViewModels.NguoiDungsViewModel

<div class="sub-banner">
    <div class="container">
        <div class="breadcrumb-area">
            <h1>Trang Chi Tiết Sản Phẩm</h1>
            <ul class="breadcrumbs">
                <li><a href="@Url.Action("Index", "Home")">Trang Chủ</a></li>
                <li class="active">Thông Tin Người Dùng</li>
            </ul>
        </div>
    </div>
</div>
<div class="user-info-section">
    <div class="container">
        <div class="row">
            <div class="col-md-8 offset-md-2">
                <div class="card">
                    <div class="card-header text-center">
                        <h3>Thông Tin Người Dùng</h3>
                    </div>
                    <div class="card-body">
                        <ul class="list-group list-group-flush">
                            <li class="list-group-item">
                                <strong>Họ và Tên: </strong>
                                <span>@Model.DangKy.HoTen</span>
                            </li>
                            <li class="list-group-item">
                                <strong>Tên Đăng Nhập: </strong>
                                <span>@Model.DangKy.TenTruyCap</span>
                            </li>
                            <li class="list-group-item">
                                <strong>Email: </strong>
                                <span>@Model.DangKy.Email</span>
                            </li>
                            <li class="list-group-item">
                                <strong>Số Điện Thoại: </strong>
                                <span>@Model.DangKy.Sdt</span>
                            </li>
                            <li class="list-group-item">
                                <strong>Loại Tài Khoản: </strong>
                                <span>@Model.LoaiTaiKhoanUsers.FirstOrDefault(x => x.IdLoaitk == @Model.DangKy.LoaiTaiKhoanId)?.TenLoaitk</span>
                            </li>
                            <li class="list-group-item">
                                <strong>Số Tiền Dư: </strong>
                                <span>@Model.DangKy.SoTien</span>
                            </li>
                        </ul>
                    </div>
                    <div class="card-footer text-center">
                        <a href="/NguoiDungs/ChinhSuaThongTin" class="btn btn-primary">Chỉnh Sửa Thông Tin</a>
                        <a href="/NguoiDungs/Logout" class="btn btn-danger">Đăng Xuất</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
